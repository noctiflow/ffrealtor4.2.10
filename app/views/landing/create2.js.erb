$("#form-continue").prop('disabled', true);
$('#secondary-form').fadeIn(3000);
$('#address-form').fadeOut(0);
$('#map').attr('src','https://www.google.com/maps/embed/v1/place?key=<%= ENV["GOOGLE_API_KEY"] %>&q=<%= @lead.address %>');

$(function(){

  $("form").submit(function(event) {
    event.preventDefault();
    var magic = $(this).serializeArray();
    $.ajax({
      type: 'PUT',
      url: '/leads/' + "<%= @lead.id %>",
      data: magic,
      dataType: "JSON",
      success: function (data) {
        console.log(data);
        location.href = "<%= landing2_path %>"
      },
      error: function (data){
        console.log(data);
      }
    });
  })
})
